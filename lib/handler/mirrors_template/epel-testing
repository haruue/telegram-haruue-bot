[epel-testing]
name=Extra Packages for Enterprise Linux ${version} - Testing - $basearch
baseurl=https://cquptmirror.com/epel/testing/${version}/$basearch
#mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=testing-epel${version}&arch=$basearch
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-${version}

